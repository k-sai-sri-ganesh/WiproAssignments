spring.application.name=Discovery-Service

server.port=8761


eureka.client.register-with-eureka=false
eureka.client.fetch-registry=false


# Disable Eureka peer replication and unnecessary features for single-node setup
eureka.server.enable-self-preservation=false
eureka.server.eviction-interval-timer-in-ms=60000
eureka.server.peer-node-read-timeout-ms=2000
eureka.server.peer-eureka-nodes-update-interval-ms=60000
eureka.server.wait-time-in-ms-when-sync-empty=0
eureka.server.enable-replication=false
